{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "docs/obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "docs/api/*.md",
          "docs/articles/**.md",
          "docs/toc.yml",
          "docs/*.md"
        ],
        "exclude": [
          "docs/obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/images/**"
        ],
        "exclude": [
          "docs/obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs/apidoc/**.md"
        ],
        "exclude": [
          "docs/obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "template": [
      "default"
    ]
  }
}